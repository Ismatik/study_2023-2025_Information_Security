    "import math\n",
    "# print(math.gcd(123,33))\n",
    "\n",
    "def euclid(a, b):\n",
    "    \"\"\"Euclid algorithm 0 < b <= a\"\"\"\n",
    "    while a != b:\n",
    "        if a > b:\n",
    "            a = a - b\n",
    "        else:\n",
    "            b = b - a\n",
    "    return a\n",
    "\n",
    "\n",
    "\n",
    "\n",
    "euclid(123,1)"
